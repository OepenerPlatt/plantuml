      <diagramTextProvider
            id="net.sourceforge.plantuml.xcore.XcoreDiagramTextProvider"
            providerClass="net.sourceforge.plantuml.xcore.XcoreDiagramTextProvider"
            label="Text : Class diagrams generated from xcore models"
